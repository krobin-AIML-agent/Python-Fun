   "id": "094de869",
   "metadata": {},
   "outputs": [],
   "source": [
    "myString = 'My name is Ryan Mitchell. I live in Boston' ## Create a string\n",
    "myString.split('.')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "28396cd9",
   "metadata": {},
   "outputs": [],
   "source": [
    "myString.split() ## Split the string at each space"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ba7da7c7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def cleanWord(word):\n",
    "    return word.replace('.', '').lower()\n",
    "\n",
    "[cleanWord(word) for word in myString.split()] ## Clean each word in the string"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "7b00a15e",
   "metadata": {},
   "outputs": [],
   "source": [
    "[cleanWord(word) for word in myString.split() if len(cleanWord(word)) < 3] ## Clean and filter words with length less than 3"
   ]
  },
